{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "mongo",
      "image": "mongo",
      "essential": true,
      "memory": 1024
    },
    {
      "name": "chukasa",
      "image": "hirooka/chukasa:0.0.1-SNAPSHOT-DOCKER",
      "essential": true,
      "memory": 2048,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "mongo"
      ]
    }
  ]
}